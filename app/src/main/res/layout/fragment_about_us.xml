<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.AboutUsFragment">




   <include layout="@layout/default_below_tool_bar_layout"
       android:id="@+id/defaultToolBarBelowLayout"/>

   <TextView
       android:id="@+id/tvAboutUs"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_below="@+id/defaultToolBarBelowLayout"
       android:layout_alignParentLeft="true"
       android:layout_marginLeft="10dp"
       android:layout_marginTop="10dp"
       android:text="About Us"
       android:textColor="@color/text_color_black"
       android:textSize="20sp"
       android:textStyle="bold" />


   <RelativeLayout
       android:id="@+id/rlAboutUsMessageParent"
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:layout_below="@+id/tvAboutUs"
       android:gravity="center_vertical"
       android:layout_margin="10dp">

      <com.example.sagarunnati.utility.JustifiedTextView
          android:id="@+id/tvAboutUsMessage"
          android:layout_width="match_parent"
          android:layout_height="match_parent"
          android:text=" "
          android:layout_gravity="center"
          android:textColor="@color/text_color_black"
          android:textSize="12sp" />
   </RelativeLayout>
  <!-- <com.example.sagarunnati.utility.JustifiedTextView
       android:id="@+id/tvAboutUsMessage"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_below="@+id/tvAboutUs"
       android:layout_alignParentLeft="true"
       android:layout_marginLeft="10dp"
       android:layout_marginTop="10dp"
       android:text="@string/about_us_message"
       android:layout_gravity="center"
       android:textColor="@color/text_color_black"
       android:textSize="14dp" />-->

</RelativeLayout>