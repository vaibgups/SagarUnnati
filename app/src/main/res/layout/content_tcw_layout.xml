                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:layout_marginLeft="5dp"
                                    android:layout_marginRight="5dp"
                                    android:text="@string/tv_tcw_coking_others"
                                    android:textSize="11sp"
                                    android:gravity="center" />

                            </LinearLayout>
                        </LinearLayout>

                        <View
                            android:layout_width="1px"
                            android:layout_height="match_parent"
                            android:background="@color/black_overlay" />

                        <LinearLayout
                            android:layout_width="150dp"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:layout_weight="1"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/tvTCWAdpTonnage"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:layout_marginLeft="5dp"
                                    android:layout_marginRight="5dp"
                                    android:text="@string/tv_tcw_tonnage"
                                    android:textSize="11sp"
                                    android:gravity="center" />

                                <View
                                    android:layout_width="1px"
                                    android:layout_height="match_parent"
                                    android:background="@color/black_overlay" />

                                <TextView
                                    android:id="@+id/tvTCWAdpTeus"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:layout_marginLeft="5dp"
                                    android:layout_marginRight="5dp"
                                    android:text="@string/tv_tcw_teus"
                                    android:textSize="11sp"
                                    android:gravity="center" />

                            </LinearLayout>


                        </LinearLayout>

                        <View
                            android:layout_width="1px"
                            android:layout_height="match_parent"
                            android:background="@color/black_overlay" />


                        <TextView
                            android:id="@+id/tvTCWAdpOthersMiscCarg"
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:text="@string/tv_tcw_others_misc_carg"
                            android:textSize="11sp"
                            android:gravity="center" />


                        <View
                            android:layout_width="1px"
                            android:layout_height="match_parent"
                            android:background="@color/black_overlay" />


                        <TextView
