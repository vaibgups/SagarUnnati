                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:text="@string/tv_tcw_total"
                            android:textSize="11sp"
                            android:gravity="center" />


                    </LinearLayout>

                    <View
